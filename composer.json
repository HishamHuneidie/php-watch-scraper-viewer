{
    "name": "hisham/php-viewer",
    "description": "App that shows php.watch in a better form",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hisham",
            "email": "hhuneidie@gmail.com"
        }
    ],
    "require": {
        "php": "8.2.*",
        "symfony/framework-bundle": "^7.2",
        "symfony/dotenv": "^7.2",
        "monolog/monolog": "^3.8",
        "symfony/runtime": "^7.2",
        "symfony/yaml": "^7.2",
        "symfony/console": "^7.2",
        "symfony/stopwatch": "^7.2",
        "ramsey/uuid": "^4.7",
        "symfony/serializer-pack": "^1.3",
        "twig/twig": "^3.17",
        "symfony/twig-bundle": "^7.2",
        "symfony/browser-kit": "^7.2",
        "symfony/http-client": "^7.2",
        "symfony/css-selector": "^7.2",
        "symfony/asset": "^7.2",
        "symfony/webpack-encore-bundle": "^2.2"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "hishamhuneidie/colored-terminal": "1.*"
    }
}
