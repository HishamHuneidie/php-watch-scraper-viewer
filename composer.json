{
    "name": "hisham/code-analyzer",
    "description": "App that implements PHPStand, PSR12, good practices, PHPCS fixer, PHPCS, PHPMD, GrumPHP and other technologies",
    "type": "project",
    "autoload": {
        "psr-4": {
            "CodeAnalyzer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hisham",
            "email": "hhuneidie@gmail.com"
        }
    ],
    "require": {
        "php": "8.2.*",
        "symfony/framework-bundle": "^7.2",
        "symfony/dotenv": "^7.2",
        "monolog/monolog": "^3.8",
        "symfony/runtime": "^7.2",
        "symfony/yaml": "^7.2",
        "symfony/console": "^7.2",
        "symfony/stopwatch": "^7.2",
        "ramsey/uuid": "^4.7",
        "symfony/serializer-pack": "^1.3",
        "twig/twig": "^3.17",
        "symfony/twig-bundle": "^7.2"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
